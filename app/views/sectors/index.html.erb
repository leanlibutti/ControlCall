<p id="notice"><%= notice %></p>

<h1>Listing Sectors</h1>

<table>
  <thead>
    <tr>
     
    </tr>
  </thead>

  <tbody>
    <% @sectors.each do |sector| %>
      <tr>
        <td><%= sector.name %></td>
        <td><%= link_to 'Show', sector %></td>
        <td><%= link_to 'Edit', edit_sector_path(sector) %></td>
        <td><%= link_to 'Destroy', sector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sector', new_sector_path %>


<div class="panel panel-success">
  <div class="panel-heading">
    <h3 class="panel-title">Sectores
  </div>
</div>

<div class="panel panel-success">
  <div class="panel-body">
    <div class="row">
      <table class="table table-bordered">
          <thead>
            <tr>
              <th>Nombre</th>
              <th colspan="3"></th> 
            </tr>
          </thead>
          <% @sectors.each do |sector| %>
            <tbody>
              <tr>
                <td><%= sector.name %></td>
                <td><%= link_to 'Ver', sector %></td>
                <td><%= link_to 'Editar', edit_sector_path(sector) %></td>
                <td><%= link_to 'Eliminar', sector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
             </tr>
            </tbody>
          <%end%>
      </table>
    </div>
  </div>
  <%= link_to 'Nuevo Sector', new_sector_path %>
</div>
 
  