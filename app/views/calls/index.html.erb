<p id="notice"><%= notice %></p>

<h1>Listing Calls</h1>

<table>
  <thead>
    <tr>
      <th>Incoming</th>
      <th>Outgoing</th>
      <th>Call date</th>
      <th>Call time</th>
      <th>Extension</th>
      <th>Dial number</th>
      <th>Ring</th>
      <th>Duration</th>
      <th>Acc code</th>
      <th>Cd</th>
      <th>Cost</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calls.each do |call| %>
      <tr>
        <td><%= call.incoming %></td>
        <td><%= call.outgoing %></td>
        <td><%= call.call_date %></td>
        <td><%= call.call_time %></td>
        <td><%= call.extension %></td>
        <td><%= call.dial_number %></td>
        <td><%= call.ring %></td>
        <td><%= call.duration %></td>
        <td><%= call.acc_code %></td>
        <td><%= call.cd %></td>
        <td><%= call.cost %></td>
        <td><%= link_to 'Show', call %></td>
        <td><%= link_to 'Edit', edit_call_path(call) %></td>
        <td><%= link_to 'Destroy', call, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Call', new_call_path %>
